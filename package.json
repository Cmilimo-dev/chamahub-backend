  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "vite",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "build:analyze": "vite build && open dist/stats.html",
    "build:prod": "vite build --mode production",
    "build:production": "npm run security:audit && npm run build:prod",
    "lint": "eslint .",
    "preview": "vite preview",
    "test:perf": "npm run build && npx serve dist -s",
    "audit:security": "npm audit",
    "audit:fix": "npm audit fix",
    "security:audit": "node scripts/security-audit.js",
    "security:check": "npm run security:audit && npm audit",
    "clean": "rm -rf dist node_modules/.vite android/app/build",
    "clean:all": "npm run clean && rm -rf node_modules",
    "precommit": "npm run lint && npm run build",
    "android:setup": "npm run build && npx cap sync android",
    "android:build": "./scripts/build-android.sh",
    "android:build:prod": "npm run build:prod && npx cap sync android && cd android && ./gradlew assembleRelease",
    "android:build:bundle": "npm run build:prod && npx cap sync android && cd android && ./gradlew bundleRelease",
    "android:run": "npx cap run android",
    "android:open": "npx cap open android",
    "android:debug": "cd android && ./gradlew assembleDebug",
    "android:release": "cd android && ./gradlew assembleRelease",
    "android:clean": "cd android && ./gradlew clean",
    "cap:doctor": "npx cap doctor",
    "verify-migration": "node verify-migration.js",
    "deploy:prepare": "npm run security:check && npm run build:production",
    "deploy:android": "npm run deploy:prepare && npm run android:build:bundle"
  },
